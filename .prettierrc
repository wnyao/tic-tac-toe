{
  "arrowParens": "always",
  "bracketSpacing": true,
  "cursorOffset": -1,
  "endOfLine": "lf",
  "insertPragma": false,
  "parser": "typescript",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "printWidth": 120,
  "proseWrap": "preserve",
  "rangeStart": 0,
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false,
  "overrides": [
    {
      "files": ["*.json", ".prettierrc"],
      "options": { "parser": "json" }
    }
  ],
  "importOrder": ["^(react/(.*)$)|^(react$)", "<THIRD_PARTY_MODULES>", "", "^@/(.*)$", "^[../../]", "^[../]", "^[./]"]
}
